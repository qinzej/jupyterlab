    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0-alpha.0",
    "@jupyterlab/apputils": "^3.1.0-alpha.0",
    "@jupyterlab/csvviewer": "^3.1.0-alpha.0",
    "@jupyterlab/docregistry": "^3.1.0-alpha.0",
    "@jupyterlab/documentsearch": "^3.1.0-alpha.0",
    "@jupyterlab/mainmenu": "^3.1.0-alpha.0",
    "@lumino/datagrid": "^0.6.0",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.1"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "^0.17.7",
    "typescript": "~3.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
